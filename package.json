{
  "name": "expo-app-with-expo-router-tailwind-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "format": "npx @biomejs/biome format src --write",
